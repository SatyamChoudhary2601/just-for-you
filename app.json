{
  "expo": {
    "name": "just-for-you",
    "slug": "just-for-you",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.akanksha.crush"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/new.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.satyam_mj.justforyou"
    },
    "web": {
      "favicon": "./assets/new.png"
    }
  }
}
